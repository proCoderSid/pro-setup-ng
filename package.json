{
  "name": "pro-setup-ng",
  "version": "0.0.0",
  "scripts": {
    "prepare": "npm i @angular/cli@16.2.0 npkill prettier lint-staged husky npm-run-all pm2 jscpd @compodoc/compodoc@1.1.21 -g && husky install",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "posttest": "npm run format",
    "format": "prettier --log-level log --write \"**/*.{html,css,scss,less,js,jsx,ts,tsx,json,md,mdx,yaml}\" --config .prettierrc",
    "compodoc:build": "compodoc -p tsconfig.doc.json -d dist/docs -n ProCoderSid --theme Vagrant --hideGenerator",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "duplicate": "jscpd src --min-tokens=100"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "validator": "^13.11.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular/cli": "^16.2.6",
    "@angular/compiler-cli": "^16.2.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@compodoc/compodoc": "^1.1.21",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "typescript": "~5.1.3"
  },
  "lint-staged": {
    "*.{html,css,scss,less,js,jsx,ts,tsx,json,md,mdx,yaml}": "prettier --write"
  }
}
